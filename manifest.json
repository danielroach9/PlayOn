{
	"name": "TheNextEpisode",
	"manifest_version": 2,
	"version": "1.0",
	"description": "Automates going to the next available episode when streaming content",
	"content_scripts": [
		{
			"matches": [
				"https://kissanime.to/Anime/*",
				"http://www.toonova.com/*"
			],
			"js": ["content.js", "bootstrap.min.css"]
		}
	],
	"browser_action": {
		"default_icon": "TNE.png",
		"default_popup": "window.html"
	}
}