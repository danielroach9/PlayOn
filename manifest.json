{
	"name": "TheNextEpisode",
	"manifest_version": 2,
	"version": "1.0",
	"description": "Automates going to the next available episode when streaming content",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["content.js"]
		}
	],
	"browser_action": {
		"default_icon": "TNE.png",
		"default_popup": "window.html"
	}
}